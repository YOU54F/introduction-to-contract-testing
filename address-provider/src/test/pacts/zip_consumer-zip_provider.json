{
  "consumer": {
    "name": "zip_consumer"
  },
  "interactions": [
    {
      "description": "A request for location data",
      "providerStates": [
        {
          "name": "the zip code does not exist"
        }
      ],
      "request": {
        "method": "GET",
        "path": "/us/99999"
      },
      "response": {
        "status": 404
      }
    },
    {
      "description": "A request for location data",
      "providerStates": [
        {
          "name": "the zip code exists"
        }
      ],
      "request": {
        "method": "GET",
        "path": "/us/90210"
      },
      "response": {
        "body": {
          "country": "United States",
          "countryAbbreviation": "US",
          "places": [
            {
              "placeName": "Beverly Hills",
              "state": "California",
              "stateAbbreviation": "CA"
            }
          ],
          "zipCode": "90210"
        },
        "headers": {
          "Content-Type": "application/json; charset=UTF-8"
        },
        "matchingRules": {
          "body": {
            "$.country": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.countryAbbreviation": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.places": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type",
                  "min": 1
                }
              ]
            },
            "$.places[*].placeName": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.places[*].state": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.places[*].stateAbbreviation": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.zipCode": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          },
          "header": {
            "Content-Type": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "regex",
                  "regex": "application/json(;\\s?charset=[\\w\\-]+)?"
                }
              ]
            }
          }
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pact-jvm": {
      "version": "4.2.6"
    },
    "pactSpecification": {
      "version": "3.0.0"
    }
  },
  "provider": {
    "name": "zip_provider"
  }
}
